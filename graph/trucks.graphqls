type Truck {
 id: ID!
 plateNo: String!
 isDeleted: Boolean
 createdAt: Int!
 updatedAt: Int!
 page: Int!
 first: Int!
}

type Query {
 paginatedTrucks: [Truck!]!
}

type Mutation {
 saveTruck(id: ID, plateNo: String!): Truck!
 updateTruck(id: ID, plateNo: String!): Truck!
 deleteTruck(id:ID): Response!
}